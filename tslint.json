{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "jsRules": {
    "object-literal-sort-keys": false,
    "trailing-comma": [true, { "multiline": "always", "singleline": "never" }]
